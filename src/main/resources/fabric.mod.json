    ]
  },
  "mixins": [
    "polydex.mixins.json"
  ],
  "accessWidener": "polydex.accesswidener",
  "depends": {
    "fabricloader": ">=0.12.6",
    "fabric": ">=0.46.1",
    "minecraft": ">=1.20"
