{
  "schemaVersion": 1,
  "id": "polydex2",
  "version": "${version}",
  "name": "Polydex",
  "description": "Target block/entity tooltip and recipes in one ui mod for servers!",
  "authors": [
    "Patbox"
  ],
  "contact": {
    "homepage": "https://pb4.eu",
    "sources": "https://github.com/Patbox/polydex"
  },
  "license": "LGPLv3",
  "environment": "*",
  "icon": "data/polydex/icon.png",
  "entrypoints": {
